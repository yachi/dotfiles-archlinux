[color]
	ui = auto
[user]
	name = Yachi Lo
	email = yaachi@gmail.com
[push]
	default = simple
[core]
	excludesfile = /home/yachi/.gitignore
	editor = nvim
	pager = /usr/share/git/diff-highlight/diff-highlight | less -FRX
[tag]
	sort = version:refname
[alias]
  dsf = "!f() { [ \"$GIT_PREFIX\" != \"\"  ] && cd "$GIT_PREFIX"; git diff --color $@ | diff-so-fancy | less --tabs=4 -RFX;  }; f"
  up = "!f() { python3 /home/yachi/.local/lib/python3*/site-packages/PyGitUp/gitup.py; }; f"
